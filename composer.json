{
    "name": "anwar/dynamic-roles",
    "description": "Complete dynamic role and permission management package with caching and API support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anwar",
            "email": "anwar@gunma.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Anwar\\DynamicRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anwar\\DynamicRoles\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anwar\\DynamicRoles\\DynamicRolesServiceProvider"
            ],
            "aliases": {
                "DynamicRoles": "Anwar\\DynamicRoles\\Facades\\DynamicRoles"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
